{
  "schemaVersion": 1,
  "id": "statues",
  "version": "${version}",
  "name": "Statues",
  "description": "Adds posable Statues",
  "authors": [],
  "contact": {},
  "license": "MIT",
  "icon": "assets/statues/icon.png",
  "environment": "*",
  "entrypoints": {
    "fabric-datagen": [
      "rings_of_saturn.github.io.statues.client.StatuesDataGenerator"
    ],
    "client": [
      "rings_of_saturn.github.io.statues.client.StatuesClient"
    ],
    "main": [
      "rings_of_saturn.github.io.statues.Statues"
    ],
    "cardinal-components": [
      "rings_of_saturn.github.io.statues.entity.components.ModComponents"
    ]
  },
  "mixins": [
    "statues.mixins.json",
    {
      "config": "statues.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  },
  "custom": {
    "cardinal-components": [
      "statues:slim",
      "statues:right_arm_angle",
      "statues:left_arm_angle",
      "statues:right_leg_angle",
      "statues:left_leg_angle",
      "statues:head_angle",
      "statues:body_angle"
    ]
  }
}
